    <Script type="xscript5.0"><![CDATA[include "Lib::Comm.xjs";
//폼 로드
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{	
	Iject.formOnload(obj);
	var nRow = this.ds_vacation.addRow();
	this.ds_search.setColumn(nRow,"searchCondition","all");	
	
	//현재날짜 등록
	var today = new Date();
	var year = today.getFullYear().toString().padLeft(4,"0");
	var month = (today.getMonth()+1).toString().padLeft(2,"0");
	var day = today.getDate().toString().padLeft(2,"0");
	
	this.Div00.Div00.Div03.Calendar00.set_value(year+month+day);
	this.Div00.Div00.Div01.Calendar00.set_value(year+month+day);
	this.Div00.Div00.Div03.Calendar01.set_value(year+month+day);
	

	this.fn_search();
	
} 

//초기화
this.Button00_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{	
	this.ds_vacation.reset();
}



//등록
this.Button01_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{	
	
	this.fn_save("save");
	
	
	
}

//추가
this.Button05_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{	
	 var nRow = this.ds_vacation.addRow();
	 
	 //현재날짜 등록
	var today = new Date();
	var year = today.getFullYear().toString().padLeft(4,"0");
	var month = (today.getMonth()+1).toString().padLeft(2,"0");
	var day = today.getDate().toString().padLeft(2,"0");
	
	//추가버튼 클릭시 현재날짜 셋팅
	this.Div00.Div00.Div03.Calendar00.set_value(year+month+day);
	this.Div00.Div00.Div01.Calendar00.set_value(year+month+day);
	this.Div00.Div00.Div03.Calendar01.set_value(year+month+day);
}
//삭제
this.fn_delete = function(obj:Button, e:nexacro.ClickEventInfo)
{
	var nRow = this.ds_vacation.rowposition;
	this.ds_vacation.deleteRow(nRow);
	var rtnValue = Iject.confirm("삭제 하시겠습니까?");
	
	if(rtnValue)
	{
		this.fn_save("delete");
	}
	else
	{
		this.fn_search();
	}
}	


this.fn_gridClick = function(obj:Grid, e:nexacro.GridClickEventInfo){
	
		if(e.col == '0'){
			this.Div00_Div00_Div00_Button00_onclick();
		}
}

