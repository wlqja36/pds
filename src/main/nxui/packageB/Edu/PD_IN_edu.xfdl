//저장
this.fn_save = function(strid)
{
	this.ds_eduselect.setColumn(this.ds_eduselect.rowposition, "Edit02", this.Edit02.value);
    
	var oDatas =  {
			  svcid : strid, //strid에 적힌 문자열을 서비스아이디로 저장
			  sController : "modifyEdu.do",			 
			  inds :["ds_input=ds_eduselect:U"],
			  outds :[
				 "ds_eduselect=output"
			     ],			     
			  args  : [				
			  ],
			  bAsync : true,    // 비동기여부 (true : async  false: sync)
			  nDataType : 0,   // (0: XML타입, 1: 이진 타입, 2: SSV 타입)
			  bCompress :false,	 //	
			}; 

    //공통 transaction 함수 호츨   //function callback 
	Iject.transaction(this,oDatas,function(){   
		if(this.errorcode < 0)
		{
		   Iject.alert(this.errorMsg,"알림");			
		   return;
		}			
	      
	       var pThis = this.fobj;   //form object
	       
	       trace("svcid: "+strid);
	 
		    switch(strid)
	      {
	         case "edu_save" : 
				alert("저장완료");
				break;
				
	         case "edu_delete" : 
	            alert("삭제완료");
				break; 
	      } 
	    
	});
}



