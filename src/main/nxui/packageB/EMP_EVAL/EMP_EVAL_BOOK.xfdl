//저장
this.fn_save = function(strid)
{

	var oDatas =  {
			  svcid : strid,
			  sController : "empEvalBookModifyVO.do",			 
			  inds :["ds_input=ds_emp_eval_book:U"],
			  outds :[
				 "ds_emp_eval_book=output1"
			     ],			     
			  args  : [				
			  ],
			  bAsync : true,   
			  nDataType : 0,   
			  bCompress :false,	 		  
	}; 
	

	
	Iject.transaction(this,oDatas,function(){   
 
		if(this.errorcode < 0)
		{
		   Iject.alert(this.errorMsg,"알림");			
		   return;
		}			
	      
	       var pThis = this.fobj;   //form object
	       
	       
	       if(pThis.confirmStr == "insert"){
				Iject.alert("추가 완료");
	       }else if(pThis.confirmStr == "update"){
				Iject.alert("수정 완료");
	       }
	       
	      switch(strid)
	      {
			 case "insert" :
				Iject.alert("삽입완료");
				break;
	         case "save" : 
				Iject.alert("저장완료");
				break;
				
	         case "delete" : 
	            Iject.alert("삭제완료");
				break; 
	      }
	    
	    pThis.g_currentRow = pThis.Grid00.currentrow;
	    pThis.btn_search_onclick();
		
	});
	
	
}

