// 		this.close();
// 		var objParam = {param1:this.ds_emp.getColumn};
}


this.fn_popupCallback = function(strPopupID, strReturn)
{
    if(strReturn == undefined){
        return;
    }

    if(strPopupID == "chf_popup1"){
        this.alert("Return Value: " + strReturn);
    }
};
]]></Script>
