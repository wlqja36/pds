    <Script type="xscript5.0"><![CDATA[
include "Lib::common.xjs";



this.Button01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.close();
}


this.empSearch_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.btn_sc_onclick();
	
}

this.btn_reset_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	this.reload();
}

this.btn_sc_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	this.ds_search.clearData();
	var nRow = this.ds_search.addRow();
	var searchCondition = this.sta_01.value;
	var searchKeyword = this.Edit05.value;
	
	this.ds_search.setColumn(nRow,"searchCondition",searchCondition);
	this.ds_search.setColumn(nRow,"searchKeyword",searchKeyword);
	this.gfn_transaction(this,
 						"vacationCodeSearch",
 						"vacationSelectSearchVO.do",
 						"ds_search=ds_search",
 						"ds_vacation=ds_output",
 						"",
 						"transaction_callback"
 						);
}

this.transaction_callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode < 0)
	{
		this.alert(sSvcId + " : " + sErrorMsg);
		return;
	}
	
}


//입력
this.Button00_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
		var vacation_code_id = this.ds_vacation.getColumn(this.ds_vacation.rowposition,"vacation_code_id");
		application.gds_vacationCodeId.addRow();
		application.gds_vacationCodeId.setColumn(application.gds_vacationCodeId.rowposition,"vacation_code_id",vacation_code_id);
		this.close();
	//	var objParam = {param1:this.ds_vacation.getColumn};
		
// 		var re_empno = this.ds_emp.getColumn(this.ds_emp.rowposition, "emp_no");	
// 		application.gds_reempno.addRow();
