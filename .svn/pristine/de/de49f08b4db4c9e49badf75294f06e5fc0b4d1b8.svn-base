// 트랜잭션 콜백
this.transaction_callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode < 0)
	{
		this.alert(sSvcId + " : " + sErrorMsg);
		this.alert("오류입니다. 관리자에게 문의하십시오");
		return;
	}
	
	
	switch(sSvcId){
		case "selectEmp":
			if(this.ds_emp.getColumn(this.ds_emp.rowposition,"marriage")=="Y")
			{
				this.Calendar00.set_enable("true");
			}
			else
			{
				this.Calendar00.set_enable("false");
			}
			if(this.ds_emp.getColumn(this.ds_emp.rowposition,"householder")=="Y")
			{
				this.Edit11.set_enable("false");
				this.Edit12.set_enable("false");
			}
			else
			{
				this.Edit11.set_enable("true");
				this.Edit12.set_enable("true");
			}
		
		break;
		case "updateAdm":
			this.alert("수정이 완료되었습니다.");
			this.close();
		break;
	
	}
}

