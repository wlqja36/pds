    <Script type="xscript5.0"><![CDATA[include "Lib::common.xjs";

this.confirmStr = "";
var rowNum = "";
var strSwitch = "";
var idxCode="";

// 폼 온로드
this.YJB_CCMM_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.ds_code.clearData();
	this.ds_code.addRow();
	this.enable_nonEdit();

	this.gfn_transaction(this,
						"selectDetailCode",
						"com/YJB_selectDetailCode.do",
						"",
						"ds_code=ds_output",
						"",
						"transaction_callback"
						);

	this.btn_cd_search_onclick();					
}

// 검색 버튼
this.btn_cd_search_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	this.combo_cd_class_onitemchanged();
}

// 전체 분류 선택 시 자동 조회
this.combo_cd_class_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	idxCode=application.gds_idx.getColumn(application.gds_idx.rowposition,"idxCode");
	application.gds_idx.clearData();

	var masterCode;
	
	if(this.combo_cd_class.value == ' '){
		masterCode = null;
	}else{
		masterCode = this.combo_cd_class.value;
	}
	
	var detailCode = this.edt_cd_dCode.value;
	var detailCodeName = this.edt_cd_codeName.value;
	
	this.ds_search.clearData();
	var rowIdx = this.ds_search.addRow();
	this.enable_nonEdit();
	
	
	if(idxCode!=null)
	{
		var strParam = "detailCode="+idxCode;
		
		this.gfn_transaction(this,
					"selectDRnum",
					"com/YJB_selectDRnum.do",
					"",
					"ds_rnum=ds_output",
					strParam,
					"transaction_callback"
					);
	}
	
	this.ds_search.setColumn(0, "searchCondition", masterCode);
	this.ds_search.setColumn(0, "searchKeyword", detailCode);
	this.ds_search.setColumn(0, "searchUseYn", detailCodeName);

	
	this.gfn_transaction(this,
						"detailList",
						"com/YJB_selectDetailList.do",
						"ds_search=ds_search",
						"ds_detailCodeList=ds_output",
						"",
						"transaction_callback"
						);
}



// 초기화 버튼
this.btn_cd_searchReset_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.reload();
}

// 편집창 활성화
this.enable_edit = function()
{
	this.edt_cd_dCodeName.set_enable("true");
	this.edt_cd_codeName_eng.set_enable("true");
	this.edt_cd_codeEtc.set_enable("true");
	this.edt_cd_codeEtc2.set_enable("true");
	this.edt_cd_dCodeInfo.set_enable("true");
	this.combo_cd_use.set_enable("true");
	this.edt_cd_sortOrder.set_enable("true");
	this.edt_cd_dCodeName.setFocus();
}

// 편집창 비활성화
this.enable_nonEdit = function()
{
	this.combo_cd_class1.set_enable("true");
	this.edt_cd_dCode1.set_enable("false");
	this.edt_cd_dCodeName.set_enable("true");
	this.edt_cd_codeName_eng.set_enable("true");
	this.edt_cd_codeEtc.set_enable("true");
	this.edt_cd_codeEtc2.set_enable("true");
	this.edt_cd_dCodeInfo.set_enable("true");
	this.combo_cd_use.set_enable("true");
	this.edt_cd_sortOrder.set_enable("true");
}
// 신규버튼
this.btn_cd_add_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	var masterCodeValue = this.combo_cd_class.value;
	
	this.ds_detailCodeList.addRow();
	this.combo_cd_use.set_value("Y");

	
	
// 	this.alert(this.ds_detailCodeList.getColumn(this.ds_detailCodeList.rowposition, "registId"));
// 	this.alert(this.ds_detailCodeList.getColumn(this.ds_detailCodeList.rowposition, "updateId"));
	
	this.enable_edit();
	
	this.edt_cd_dCode1.set_enable("true");
	//this.Combo02.set_enable("true");
	this.combo_cd_class1.set_value(masterCodeValue);
	this.edt_cd_dCode1.setFocus();
	
}



// 저장 버튼

this.btn_cd_save_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	if(this.combo_cd_class1.index == -1){
		this.alert("분류코드를 확인해주세요");
		this.combo_cd_class1.setFocus();
		return;
	}else if(this.edt_cd_dCode1.value == null || this.edt_cd_dCode1.getLength() != 4){
		this.alert("상세코드를 확인해주세요");
		this.edt_cd_dCode1.setFocus();
		return;
	}else if(this.edt_cd_dCodeName.value == null){
		this.alert("상세코드명을 입력해주세요");
		this.edt_cd_dCodeName.setFocus();
		return;
	}
	
	var confirmStr = this.confirm("저장하시겠습니까?");
	
	if(confirmStr){
		var id = application.gds_User.getColumn(application.gds_User.rowposition, "id");
		if(id == null) {
		id = "ID";
		}
		this.ds_detailCodeList.setColumn(this.ds_detailCodeList.rowposition, "regId", id);
		this.code_transaction("saveCode");
	}
}


// 코드 정보 저장/수정/삭제 시 트랜잭션
this.code_transaction = function(sSvcid)
{
	//var updateId = application.gds_User.getColumn(application.gds_User.rowposition, "id"); 
	//var strParam = "updateId=" + updateId;
	
	rowNum = this.ds_detailCodeList.rowposition;
	
	var strSvcid = sSvcid;
	var strController = "com/YJB_editDetailCode.do";
	
	this.gfn_transaction(this,
						strSvcid,
						strController,
						"ds_input=ds_detailCodeList:U",
						"",
						"",
						"transaction_callback"
						);
	
}


// 트랜잭션 콜백
this.transaction_callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode < 0)
	{
		this.alert(sSvcId + " : " + sErrorMsg);
		return;
	}
	
	switch(sSvcId){
		
		case "detailList":
			var setNum = application.gds_idx.getColumn(application.gds_idx.rowposition,"rnum");

			application.gds_idx.clearData();
			
				if (this.ds_detailCodeList.getRowCount() == 0)
				{
					this.ds_detailCodeList.filter("");
					this.alert("조회할 자료가 없습니다.");
					
					return;
				}
				else
				{
					
// 					this.fv_iServerRowCount = this.ds_detailCodeList.getRowCount();
// 					this.fn_SetStyle();
// 					this.fn_SetPageNo();
					
					
				} 

		
			this.enable_nonEdit();
			this.fn_SetStyle();
			this.grd_plaintiff.selectRow(rowNum);
			break;
	
		case "saveCode":
			if(this.confirmStr == "저장"){
				application.gds_idx.addRow();
				application.gds_idx.setColumn(0,"idxCode",this.ds_detailCodeList.getColumn(this.ds_detailCodeList.rowposition,"detailCode"));
				
				this.btn_clear_onclick();
				this.alert("저장완료");
			}else if(this.confirmStr == "수정"){
				strSwitch = "수정";
				this.Combo00_onitemchanged();
				this.alert("수정완료");
			}
			
			break;
			
		case "codeChecker": 
			var check = this.ds_check.getColumn(this.ds_check.rowposition, "detailCode");
			trace("체크:"+check);
			if(check != "NONE"){
				this.alert("중복 코드입니다.");
				this.edt_cd_dCode1.set_value(null);
				this.edt_cd_dCode1.set_text("");
				this.edt_cd_dCode1.setFocus();
			}
			break;
		case "deleteCode":
			alert("삭제완료");
			break;
		case "selectDRnum":
			application.gds_idx.addRow();
			application.gds_idx.setColumn(0,"rnum",this.ds_rnum.getColumn(this.ds_rnum.rowposition,"rnum"));
			trace("DRnum:"+application.gds_idx.getColumn(0,"rnum"));
			break;
	}
	
}

// 삭제 버튼
this.btn_cd_delete_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	var confirmStr = this.confirm("삭제하시겠습니까?");
	
	if(confirmStr){
		this.ds_detailCodeList.deleteRow(this.ds_detailCodeList.rowposition);
		this.code_transaction("deleteCode");
	}
}




