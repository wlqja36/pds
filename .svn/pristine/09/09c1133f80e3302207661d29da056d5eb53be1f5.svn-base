    <Script type="xscript5.0"><![CDATA[
include "Lib::common.xjs";

this.member002_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	this.setInitial();
}


this.setInitial = function()
{
	this.ds_emp.setColumn(0,"householder","N");
	this.ds_emp.setColumn(0,"disability","N");
	this.ds_emp.setColumn(0,"marriage","N");
	this.Calendar00.set_enable("false");
}

// 아이디 검사
this.Edit05_canchange = function(obj:Edit,  e:nexacro.ChangeEventInfo)
{
	var idValue = this.Edit05.value;
	
	var strParam = "emp_id=" + idValue;
	
	this.gfn_transaction(this,
						"idChecker",
						"administrator/selectId.do",
						"",
						"ds_check=ds_output",
						strParam,
						"transaction_callback"
						);
}


// 우편 번호 검색하기
this.btn_execute_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.Plugin00.callMethod("Navigate2", "http://localhost:8080/pd_insa/jsp/zipcodeSearch.jsp"); 
}

// 우편 번호 검색 후 값 받기
this.Plugin00_TitleChange = function()
{
	
	if(arguments[0] != "TITLE"){
		var postArr = arguments[0].split("_");
	
		this.Edit01.set_value(postArr[1]);
		
		this.Edit10.set_enable(true);
	}
	
}

