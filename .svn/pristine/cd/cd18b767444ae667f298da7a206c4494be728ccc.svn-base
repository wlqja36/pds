//등록
this.fn_save = function(strid)
{

	this.ds_confirm.setColumn(this.ds_confirm.rowposition, "salary_reg_user_no", this.edt_btr_no00.value);
	this.ds_confirm.setColumn(this.ds_confirm.rowposition, "salary_acc_user_no", this.edt_btr_no04.value);
	this.ds_confirm.setColumn(this.ds_confirm.rowposition, "salary_mod_user_no", this.edt_btr_no05.value);
	
	var oDatas =  {
			  svcid : strid,
			  sController : "sbModifyVO.do",			 
			  inds :["input1=ds_confirm:U"],
			  outds :[
				 "ds_confirm=output1"
			     ],			     
			  args  : [				
			  ],
			  bAsync : true,    // 비동기여부 (true : async  false: sync)
			  nDataType : 0,   // (0: XML타입, 1: 이진 타입, 2: SSV 타입)
			  bCompress :false,	 //			  
			}; 
		 //trace(" this.confirm: " + this.confirm.saveXML());


    //공통 transaction 함수 호츨   //function callback 
	Iject.transaction(this,oDatas,function(){   
		/*  
		*   callback argument
		*   @param : {string}    this.svcid       서비스 ID
		*   @param : {object}    this.fobj        : 현재 form object
		*   @param : {number}    this.errorcode  : 서비스 에러 코드
		*   @param : {string}    this.errormsg   : 서비스 에러 메세지
		*/    
		if(this.errorcode < 0)
		{
		   Iject.alert("필수입력을 확인해주세요.","알림");			
		   return;
		}			
	      
	       var pThis = this.fobj;   //form object       
	       
	       
	      switch(strid)
	      {
			case "modify" :
				Iject.alert("수정완료");
				break;
	      
	      
	         case "save" : 
				Iject.alert("등록완료");
				
				var salary = Iject.confirm("급여를 등록하시겠습니까?");
				if(salary){
					 pThis.fn_salary_gogo();
					 }
				 break;
				 
	         case "delete" : 
	            Iject.alert("삭제완료");
				break; 
				
			 
	      }
	      
			
		  this.fn_search();
		  
	});
}

