 //팝업 콜백함수
  this.fn_popupCallback = function(strPopupID, strReturn)
  {
      if(strReturn == undefined){
          return;
      }
  
      if(strPopupID == "Salary_book"){
          this.alert("Return Value: " + strReturn);
      }
      
    else if(strPopupID == "Salary"){
		this.alert("Return Value: " + strReturn);
		}
	else if(strPopupID == "attitude_id"){
		this.alert("Return Value: " + strReturn);
		this.edt_btr_no03.set_value(strReturn);
		}
	else if(strPopupID == "s_emp_name"){
		this.alert("Return Value: " + strReturn);
		this.edt_btr_no00.set_value(strReturn);
		}
 }

