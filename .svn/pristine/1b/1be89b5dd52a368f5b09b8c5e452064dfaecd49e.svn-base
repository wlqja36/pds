        	    trace(" ds_emp_full당 : " + pThis.ds_emp_full.saveXML());
        	    var sum = nexacro.toNumber(nexacro.toNumber(pThis.EMP_EVAL1.value) + nexacro.toNumber(pThis.EMP_EVAL2.value));
