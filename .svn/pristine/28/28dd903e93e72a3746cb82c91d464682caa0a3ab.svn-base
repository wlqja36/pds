            this.btn_cc_add.addEventHandler("onclick", this.btn_cc_add_onclick, this);
            this.btn_cc_save.addEventHandler("onclick", this.btn_cc_save_onclick, this);
            this.edt_cc_class.addEventHandler("oneditclick", this.edt_cc_class_oneditclick, this);
            this.edt_cc_class.addEventHandler("canchange", this.edt_cc_class_canchange, this);
            this.sta_cc_class.addEventHandler("onclick", this.Static06_onclick, this);
            this.Static10.addEventHandler("onclick", this.Static05_onclick, this);
            this.sta_cc_mainTable.addEventHandler("onclick", this.Static06_onclick, this);
            this.combo_cc_use.addEventHandler("onitemchanged", this.Combo00_onitemchanged, this);
            this.sta_cc.addEventHandler("onclick", this.Static00_onclick, this);
            this.btn_cc_search.addEventHandler("onclick", this.btn_cc_search_onclick, this);
            this.btn_cc_reset.addEventHandler("onclick", this.btn_cc_reset_onclick, this);

        };

        this.loadIncludeScript("YJB_CCMM001.xfdl");

       
    };
